{
  "name": "tapex",
  "version": "0.0.1",
  "description": "",
  "module": "lib/index.esm.js",
  "main": "lib/index.cjs.js",
  "browser": "lib/index.iife.js",
  "scripts": {
    "commit": "git add . && git-cz",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --cache --fix",
    "lint:style": "stylelint \"./src/**/*.{less,css}\" --config=.stylelintrc.json --fix",
    "build:tapex": "rollup --config ./build/rollup.config.js",
    "build:types": "tsc",
    "build": "rm -rf lib && yarn build:tapex && yarn build:types"
  },
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qunzi0214/Tapex.git"
  },
  "author": "Rango",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qunzi0214/Tapex/issues"
  },
  "homepage": "https://github.com/qunzi0214/Tapex#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "4",
    "autoprefixer": "^10.3.7",
    "commitizen": "^4.2.4",
    "cssnano": "^5.0.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "install-peers": "^1.0.3",
    "less": "^4.1.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.9",
    "rollup": "^2.57.0",
    "rollup-plugin-postcss": "^4.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "antd": "^4.17.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{css,scss,less,js}": "stylelint --config=.stylelintrc.json --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
