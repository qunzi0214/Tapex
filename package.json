{
  "name": "tapex",
  "version": "0.0.1",
  "description": "",
  "main": "main.tsx",
  "scripts": {
    "commit": "git add . && git-cz",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --cache --fix",
    "lint:style": "stylelint \"./src/**/*.{less,css}\" --config=.stylelintrc.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qunzi0214/Tapex.git"
  },
  "author": "Rango",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qunzi0214/Tapex/issues"
  },
  "homepage": "https://github.com/qunzi0214/Tapex#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^11.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "antd": "^4.16.13",
    "react": "^17.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{css,scss,less,js}": "stylelint --config=.stylelintrc.json --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}